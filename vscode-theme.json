    "editor.wordHighlightBackground": "#16242E33",
    "editor.wordHighlightStrongBackground": "#21333A44",
    "editorIndentGuide.background1": "#081017",
    "editorIndentGuide.activeBackground1": "#103040",

    "titleBar.activeBackground": "#05070D",
    "titleBar.activeForeground": "#E8F5F9",
    "titleBar.inactiveBackground": "#04060A",
    "titleBar.inactiveForeground": "#98A6AF",

    "activityBar.background": "#050811",
    "activityBar.foreground": "#CFEFF9",
    "activityBarBadge.background": "#8FAF8D",
    "activityBarBadge.foreground": "#05121A",

    "sideBar.background": "#05070D",
    "sideBar.foreground": "#C9DCE4",
    "sideBarSectionHeader.background": "#060814",
    "sideBarTitle.foreground": "#BFD8E6",
