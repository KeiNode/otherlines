  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Functions",
        "scope": [
          "entity.name.function",
          "variable.function",
          "support.function",
          "meta.function",
          "meta.method"
        ],
        "settings": {
          "foreground": "#BFEAFF",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Arrow functions",
        "scope": [
          "meta.function.arrow",
          "variable.other.arrow",
          "keyword.operator.arrow",
          "punctuation.definition.parameters"
        ],
        "settings": {
          "foreground": "#BFEAFF",
          "fontStyle": "bold italic"
        }
      },
      {
        "name": "Keywords",
        "scope": [
          "keyword",
          "keyword.control",
          "storage.type",
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#9FD7FF",
          "fontStyle": ""
        }
      },
      {
        "name": "Variables & Identifiers",
        "scope": [
          "variable",
          "identifier",
          "variable.other.readwrite",
          "variable.other"
        ],
        "settings": {
          "foreground": "#D6FFEA",
          "fontStyle": ""
        }
      },
      {
        "name": "Strings / Arrays / Constants (title-like)",
        "scope": [
          "string",
          "string.quoted",
          "constant.other",
          "constant.other.symbol",
          "constant.character",
          "meta.sequence",
          "meta.structure.array"
        ],
        "settings": {
          "foreground": "#CFA8FF",
          "fontStyle": ""
        }
      },
      {
        "name": "Numbers",
        "scope": [
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float"
        ],
        "settings": {
          "foreground": "#D6FFEA",
          "fontStyle": ""
        }
      },
      {
        "name": "Comments",
        "scope": [
          "comment",
          "punctuation.definition.comment",
          "string.comment"
        ],
        "settings": {
          "foreground": "#7C858C",
          "fontStyle": "italic"
        }
      }
    ],
    "[One Dark Pro Nigth]": {
      "textMateRules": [
        {
          "scope": "keyword",
          "settings": {
            "foreground": "#A7C4A0"
          }
        },
        {
          "scope": "string",
          "settings": {
            "foreground": "#EDE7D9"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "foreground": "#F8F6F0"
          }
        },
        {
          "scope": "comment",
          "settings": {
            "foreground": "#C1A68C",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "constant",
          "settings": {
            "foreground": "#7A8F77"
          }
        }
      ]
    }
  },

  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "function": {
        "foreground": "#BFEAFF",
        "fontStyle": "bold"
      },
      "method": {
        "foreground": "#BFEAFF",
        "fontStyle": "bold"
      },
      "property": {
        "foreground": "#D6FFEA"
      },
      "variable": {
        "foreground": "#D6FFEA"
      },
      "parameter": {
        "foreground": "#D6FFEA"
      },
      "keyword": {
        "foreground": "#9FD7FF"
      },
      "string": {
        "foreground": "#CFA8FF"
      },
      "number": {
        "foreground": "#D6FFEA"
      },
      "comment": {
        "foreground": "#7C858C",
        "fontStyle": "italic"
      }
    }
  }
}
